{
    "explorer.confirmDelete": false,
    "security.workspace.trust.untrustedFiles": "open",
    "workbench.colorTheme": "Tokyo Night",
    "github.copilot.editor.enableAutoCompletions": true,
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "scm.showIncomingChanges": "always",
    "scm.showOutgoingChanges": "always",
    "security.allowedUNCHosts": [
        "wsl.localhost"
    ],
    "files.autoSave": "afterDelay",
    "explorer.confirmPasteNative": false,
    "git.confirmSync": false,
    "explorer.confirmDragAndDrop": false,
    "glassit.alpha": 245,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "workbench.editorAssociations": {
        "*.copilotmd": "vscode.markdown.preview.editor",
        "{git,gitlens}:/**/*.{md,csv,svg}": "default"
    },
    "workbench.activityBar.location": "top",
    "workbench.iconTheme": "eq-material-theme-icons-palenight",
    "workbench.sideBar.location": "right",
    "window.commandCenter": false,
    "workbench.layoutControl.enabled": false,
    "window.menuBarVisibility": "compact",
    "window.titleSeparator": " | ",
    "markdown-preview-enhanced.previewTheme": "one-dark.css",
    "terminal.integrated.fontFamily": "FiraCode Nerd Font",
    "editor.fontFamily": "'FiraCode Nerd Font', Consolas, 'Courier New', monospace",
    "explorer.fileNesting.patterns": {
        "*.ts": "${capture}.js",
        "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
        "*.jsx": "${capture}.js",
        "*.tsx": "${capture}.ts",
        "tsconfig.json": "tsconfig.*.json",
        "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb",
        "*.sqlite": "${capture}.${extname}-*",
        "*.db": "${capture}.${extname}-*",
        "*.sqlite3": "${capture}.${extname}-*",
        "*.db3": "${capture}.${extname}-*",
        "*.sdb": "${capture}.${extname}-*",
        "*.s3db": "${capture}.${extname}-*"
    },
    "editor.unicodeHighlight.invisibleCharacters": false,
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "vscord.status.details.text.editing": "~/{workspace}",
    "vscord.status.details.text.notInFile": ".........not in a file",
    "vscord.status.details.text.idle": "idling~",
    "vscord.status.details.text.noWorkSpaceText": "[-]",
    "vscord.status.details.text.viewing": "~/{workspace}",
    "vscord.status.details.text.debugging": "debugging {file_name}{file_extension}",
    "vscord.status.image.large.debugging.text": "debugging a {lang} file~",
    "vscord.status.image.large.editing.text": "editing a {lang} file~",
    "vscord.status.image.large.idle.text": "idling~",
    "vscord.status.image.large.notInFile.text": "where am i :3",
    "vscord.status.image.large.viewing.text": "viewing a {lang} file~",
    "vscord.status.image.small.notInFile.text": "........",
    "vscord.status.buttons.button1.git.active.label": "Repository",
    "vscord.status.buttons.button1.git.idle.label": "Repository",
    "vscord.status.buttons.button1.git.inactive.label": "Repository",
    "vscord.status.state.text.debugging": "debugging: {workspace}",
    "vscord.status.state.text.editing": "{file_name}{file_extension} | {current_line}:{current_column}",
    "vscord.status.state.text.noWorkspaceFound": "no workspace...?",
    "vscord.status.state.text.viewing": "{file_name}{file_extension}",
}